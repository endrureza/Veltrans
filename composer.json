{
    "name": "endrureza/veltrans",
    "description": "Package for integrating laravel with midtrans",
    "keywords": [
        "laravel",
        "midtrans",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Endru Reza",
        "email": "lotusb13@gmail.com",
        "homepage": "https://endrureza.github.io"
    }],
    "support": {
        "issues": "https://github.com/endrureza/veltrans/issues",
        "source": "https://github.com/endrureza/veltrans"
    },
    "require": {
        "php": "7.*",
        "phpseclib/phpseclib": "~2.0",
        "doctrine/dbal": "~2.5",
        "laravel/framework": "5.6.*",
        "veritrans/veritrans-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "symfony/dom-crawler": "~3.1",
        "laravel/laravel": "5.6.*",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Endru\\Veltrans\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endru\\Veltrans\\Tests": "tests/unit-tests/",
            "Endru\\Veltrans\\Tests\\Extend": "tests/extend/",
            "Endru\\Veltrans\\Tests\\Traits\\": "tests/traits/"
        },
        "classmap": [
            "tests/Test.php",
            "tests/seeds/SampleSeeder.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Endru\\Veltrans\\Providers\\VeltransProvider"
            ],
            "aliases": {
                "Veltrans": "Endru\\Veltrans\\Facades\\VeltransFacade"
            }
        }
    }
}